{
  "manifest_version": 3,
  "name": "Cyber Surge",
  "description": "\"This is a browser based extension for safe dark web surfing\"",
  "version": "1",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": ["background_script.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["content_script.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "Extension"
  },
  "page_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "pageAction/index.html",
    "default_title": "Extension"
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": [
    "geolocation",
    "storage",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "http:///",
    "https:///",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "http://localhost/*",
    "https://api.mydomain.com/*",
    "https://*.myvpn.com/",
    "<all_urls>"
  ]
}
