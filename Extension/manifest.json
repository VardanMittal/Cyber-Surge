{
  "manifest_version": 3,
  "name": "Cyber Surge",
  "version": "0.1.0",
  "description": "This tools helps you to surf darkweb safely!!",
  "icons": {
    "48": "icons/Icon_48.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.google.com/search?*"],
      "js": ["urls_extraction.js"]
    }
  ],
  "permissions": ["webRequest"]
}
